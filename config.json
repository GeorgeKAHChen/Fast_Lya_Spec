{
    // Here is the parameter you can change for computing Fast Lya Spec
    // PARAMETER: Import Model
    "dyn": "model.Lorenz",
    //
    // PARAMETER: time t sequence
    // The Initial of t always be 0
    // t_rem should lower than t_max
    // and the time sequence greater than t_rem lower than t_max 
    // will be used to calculate the LE
    "t_max":1000,
    "t_rem":500,
    "delta_t":1e-1,
    // PARAMETER: group attractor: how long the sub-attractor will be calculate in one loop
    // = positive int, -1 means infinity
    "group_atta": 10000,
    // PARAMETER: time tau sequence
    // The Initial of tau always be 0
    "tau_max":1000,
    "delta_tau": 1e-1,
    // device = "cpu" for cpu based computing or "cuda" for NVIDIA gpu based computin
    "device": "cpu"
}
